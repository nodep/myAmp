RDAX ADCL,1.0000000000
RDAX ADCR,0.4466835922
WRAX REG0,0.0000000000
;------ Pot 2
;------ Pot 1
;------ Ring Mod
SKP RUN ,3
WRAX REG1,0.0000000000
SOF 0.0000000000,-1.0000000000
